{{right_sidebar_enabled = True}}
{{left_sidebar_enabled = True}}
{{extend 'layout.html'}}
{{for a in articles:}}
<article>
    {{=H2(a.title)}}
    {{=SPAN(a.author)}}
    {{=SPAN(a.created)}}
    {{=P(a.teaser)}}
    {{=A('read more', _href=URL('plugin_blog', 'articles', args=[a.id]))}}
</article>
{{pass}}
